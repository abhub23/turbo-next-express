{
  "version": 2,
  "builds": [
    {
      "src": "apps/server/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/server/dist/index.js"
    }
  ],
  "installCommand": "corepack enable && pnpm install --no-frozen-lockfile",
  "buildCommand": "pnpm --filter=apps/server... run build"
}
